    <script src="https://cdn.rawgit.com/InventPartners/bootstrap-responsive-tabs/master/jquery.responsivetabs.js"></script>
    <script>
        $(function () {
            $('.nav-tabs').responsiveTabs();
        });
    </script>


    @RenderSection("scripts", required: false)
